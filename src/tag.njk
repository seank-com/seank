---
layout: default
eleventyExcludeFromCollections: true
pagination:
    data: collections.pagedPostsByTag
    size: 1
    alias: paged
permalink: "tags/{{ paged.tagName }}/{% if paged.number > 1 %}{{ paged.number }}/{% endif %}index.html"
eleventyComputed:
    title: "Tagged with \"{{ paged.tagName | capitalize }}\""
---

<article class="uk-article">
    <h1 class="uk-article-title">Tagged with <span class="uk-label">#{{ paged.tagName | capitalize }}</span></h1>
</article>
{% include "partials/post-grid.njk" %}
{% if collections.tagList[paged.tagName] > site.paginate %}
{% include "partials/tagpaginator.njk" %}
{% endif %}